<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Fonts\Bondi.otf" />
    <None Remove="Fonts\MusticaproSemibold.otf" />
    <None Remove="Fonts\Winglet.otf" />
    <None Remove="Imag\back" />
    <None Remove="Imag\png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.0.1" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.4" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Fonts\Bondi.otf" />
    <Resource Include="Fonts\MusticaproSemibold.otf" />
    <Resource Include="Fonts\Winglet.otf" />
    <Resource Include="Imag\back" />
    <Resource Include="Imag\png" />
  </ItemGroup>

</Project>
